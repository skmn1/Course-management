<odoo>
  <data>
    <!-- explicit list view definition -->
    
    <record model="ir.ui.view" id="course_management_system.list">
      <field name="name">course_management_system list</field>
      <field name="model">course_management_system.course_management_system</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="value"/>
          <field name="value2"/>
        </tree>
      </field>
    </record>
   
   

    <!-- actions opening views on models -->
    
    <record model="ir.actions.act_window" id="course_management_system.action_window">
      <field name="name">course_management_system window</field>
      <field name="res_model">course_management_system.course_management_system</field>
      <field name="view_mode">tree,form</field>
    </record>
   



    <!-- Top menu item -->
    
    <menuitem name="course_management_system" id="course_management_system.menu_root"/>
   
    <!-- menu categories -->
    
    <menuitem name="Menu 1" id="course_management_system.menu_1" parent="course_management_system.menu_root"/>
    <menuitem name="Menu 2" id="course_management_system.menu_2" parent="course_management_system.menu_root"/>
   
    <!-- actions -->
    
    <menuitem name="List" id="course_management_system.menu_1_list" parent="course_management_system.menu_1"
              action="course_management_system.action_window"/>
    
    
  </data>
</odoo>